Class {
	#name : #Pair,
	#superclass : #Object,
	#instVars : [
		'first',
		'second'
	],
	#category : #'2tDijkstraAlgorithm'
}

{ #category : #accessing }
Pair >> first [

	^ first
]

{ #category : #accessing }
Pair >> first: anObject [

	first := anObject
]

{ #category : #accessing }
Pair >> second [

	^ second
]

{ #category : #accessing }
Pair >> second: anObject [

	second := anObject
]
